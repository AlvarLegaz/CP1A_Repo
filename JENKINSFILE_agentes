pipeline {
    agent none
    
    stages {
        stage('Windows'){
            
            agent any
            steps{
                bat '''
                hostname
                whoami
                '''
            }
        }
		stage('LinuxA'){
			
			agent{label 'linux'}
			steps{
				echo 'Hello world'
				sh '''
					hostname
					uname -a
					whoami
				'''
			}
		}
		stage('LinuxB'){
			
			agent{label 'linux'}
			steps{
				echo 'Hello world'
				sh '''
					hostname
					uname -a
					whoami
				'''
			}
		}
    }
}